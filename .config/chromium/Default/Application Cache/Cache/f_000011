                        MyAccountApp.Questions.modal.show($('#mutipleModal'));
                        return false;
                    });

                    $(".multi").change(function(){
                        var total = $(this).children(":selected").val();
                        var divisionSelect = $(".multiDivision");
                        divisionSelect.find('option').remove();
                        for(var i=1; i<=total; i++){
                            divisionSelect.append('<option value="' + i + '">' + i + '</option>');
                        }
                    });
                

                

