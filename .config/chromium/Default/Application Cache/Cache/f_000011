                        'trigger': document.querySelector('.bookmarks-widget'),
                        'hostname': 'mercadolibre.com.ar'
                    });

                    /* FIX
                        Cuando abris otro layer no se cierra el de bookmarks y viceversa
                     */
                    $('.bookmarks-widget').on('click', function () {
                        layerNotifications.hide();
                        $('#nav-header-user-switch').attr('checked', false);
                    });

                    $('.nav-icon-notifications').on('click', function () {
                        $('#nav-header-user-switch').attr('checked', false);
                    });

                    $('.nav-icon-notifications, .nav-header-user').on('click', function () {
                        bookmarksWidget.hide();
                    });
                    /* END FIX */


                    meli.notifications.init({
                        'text': {
                            'header': "Notificaciones",
                            'empty': "Por ahora, no hay nada aqu&iacute;."
                        },
                        'debug': false,
                        'webkitOnOpen': true
                    });

                    var autocompleteConfig = {
                        'searchForm': $('.nav-search'),
                        'categoryCheckbox': $('#categorySearch'),
                        'categoryCheckboxLabel': $('label[for="categorySearch"]'),
                        'maxQueryLength': 35,
                        'meliDomain': 'mercadolibre.com.ar/',
                        'searchQuery': 'http://listado.mercadolibre.com.ar/$query',
                        'suggestionsQuantity': 20,
                        'siteId': "MLA",
                        'platformId': 'ML',
                        'officialStoreFiltersEnabled': 'true',
                        'officialStoreFilterId': 'official_store',
                        'messages':{
