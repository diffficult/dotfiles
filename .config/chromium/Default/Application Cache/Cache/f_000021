		google.load("visualization", "1", {packages:["corechart"]});

		var bind = (window.addEventListener ? 'addEventListener' : 'attachEvent');
	    var prefix = (bind === 'addEventListener' ? '' : 'on');
	    window[bind](prefix + 'load', function () {

			JcorsLoader.load(
				"https://secure.mlstatic.com/org-img/ch/ui/0.13.4/chico-jquery.min.js", function() {

					$('.ch-icon-heart').on('click', function () {
						for (var key in ch.instances.layer) {
							ch.instances.layer[key].hide(); // esto anda con chico viejo.
						}
					});

					$('#notification, #account').on('click', function () {
						bookmarksWidget.hide();
					});


				},
				"//myaccount.mlstatic.com/summary/bundle__large-332c69b2df.js.gz", function() {

					var $loadingContainer = $('[data-js="container-loading"]'),
							$dataContainer = $('[data-js="data-container"]');

					// Actualiza el nickname, previene issue de nickname cacheado por appcache
					$.ajax({
						url: '/summary/userInfo',
						success: function(response) {
							if (response.redirect) {
								window.location.href = response.redirect;
								return;
							}

							$('.ml-nick-name').text(response.user.nickname);
						}
					});

					// Appendea la data del summary
					$.ajax({
						url: '/summary/data',
						success: function(data) {

							$loadingContainer.fadeOut(100, function() {
								$dataContainer.append(data);
							});
						},
						error: function(response) {

							var template;
							// Offline validation
							if (response.status === 0 && response.readyState === 0) {
								template = 	'<h1 class="myml-title clear">Resumen</h1>'+
										'<div class="ch-box-attention">'+
										'No hay buena conexi&oacute;n a internet. Rev&iacute;sala y aqu&iacute; estaremos.'+
										'</div>';

								$loadingContainer.fadeOut(100, function() {
									$dataContainer.append(template);
								});
								return;
							}

							template = 	'<h1 class="myml-title clear">Resumen</h1>'+
									'<div class="ch-box-error">'+
									'No pudimos cargar el resumen de tu cuenta. Mientras lo solucionamos, puedes visitar otras secciones.'+
									'</div>';

							$loadingContainer.fadeOut(100, function() {
								$dataContainer.append(template);
							});
						}
					});
				}
			);

		});
	</script>

