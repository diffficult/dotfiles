{
   "author": "Raymond Hill",
   "browser_action": {
      "default_icon": {
         "19": "icon19.png",
         "38": "icon38.png"
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "contentscript.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "A companion extension to uBlock Origin: this gives uBlock Origin the ability to see and filter WebSocket connection attempts.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "36.0",
   "name": "uBlock Origin WebSocket",
   "short_name": "uBO-WebSocket",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1"
}
